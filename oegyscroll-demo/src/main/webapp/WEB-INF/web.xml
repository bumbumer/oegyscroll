<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Oegyscroll demo web application</display-name>
    <filter>
       <filter-name>wicket.front</filter-name>
        <filter-class>org.apache.wicket.protocol.http.WicketFilter</filter-class>
        <init-param>
            <param-name>applicationClassName</param-name>
            <param-value>org.laughingpanda.oegyscrolldemo.OegyScrollDemoApplication</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>wicket.front</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>  
</web-app>
